{
	"name": "github-embeds",
	"version": "1.0.6",
	"description": "Embed GitHub issues, PRs, and code snippets directly in Obsidian.",
	"main": "main.js",
	"scripts": {
		"dev": "node -r dotenv/config esbuild.config.mjs",
		"build": "npm run codegen && npm run tsc && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"codegen": "graphql-codegen --config codegen.ts",
		"prettier": "prettier --write src",
		"prettier:check": "prettier --check src",
		"lint": "eslint src",
		"tsc": "tsc -noEmit -skipLibCheck"
	},
	"author": "Gino Valente",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/MrGVSV/obsidian-github-embeds.git"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "5.0.0",
		"@graphql-codegen/introspection": "4.0.0",
		"@graphql-codegen/near-operation-file-preset": "^2.5.0",
		"@graphql-codegen/typed-document-node": "^5.0.1",
		"@graphql-codegen/typescript": "4.0.1",
		"@graphql-codegen/typescript-operations": "^4.0.1",
		"@types/node": "^16.11.6",
		"@types/prismjs": "^1.26.3",
		"@types/tinycolor2": "^1.4.3",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.18.14",
		"esbuild-sass-plugin": "^2.10.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"nanoid": "^4.0.2",
		"obsidian": "latest",
		"postcss": "^8.4.26",
		"postcss-modules": "^6.0.0",
		"prettier": "3.0.0",
		"tslib": "2.4.0",
		"typescript": "^5.3.2"
	},
	"dependencies": {
		"@apollo/client": "^3.8.1",
		"apollo-link-rest": "^0.9.0",
		"graphql": "^16.8.0",
		"immer": "^10.0.2",
		"qs": "^6.11.2",
		"tinycolor2": "^1.6.0"
	}
}
